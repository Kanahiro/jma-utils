{
    "name": "jma-utils",
    "version": "1.0.0",
    "description": "utilities for Japan Meteorological Agency geospatial data",
    "main": "./dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "test": "jest",
        "build": "tsc",
        "prepare": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Kanahiro/jma-utils.git"
    },
    "author": "Kanahiro Iguchi <kanahiro.iguchi@gmail.com> (https://github.com/Kanahiro)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Kanahiro/jma-utils/issues"
    },
    "homepage": "https://github.com/Kanahiro/jma-utils#readme",
    "dependencies": {
        "axios": "^0.21.1"
    },
    "devDependencies": {
        "@types/jest": "^26.0.20",
        "@types/node": "^14.14.14",
        "jest": "^26.6.3",
        "ts-jest": "^26.5.2",
        "typescript": "^4.1.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "js"
        ],
        "transform": {
            "^.+\\.ts$": "ts-jest"
        },
        "globals": {
            "ts-jest": {
                "tsconfig": "tsconfig.json"
            }
        },
        "testMatch": [
            "**/tests/**/*.test.ts"
        ],
        "moduleNameMapper": {
            "^#/(.+)": "<rootDir>/src/$1"
        }
    }
}
